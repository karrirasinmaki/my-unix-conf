# Paths
export PATH="$PATH:/usr/bin/vendor_perl/"

# Aliases
alias gitlog='git log -20 --pretty=format:"%C(red)%h%Creset\\ %C(green)%ar%C(cyan)%d\\ %Creset%s%C(red)\\ [%cn]" --graph --decorate --all'
eval "$(thefuck-alias)"

alias codegrep='grep -r --include=\*.{js,ts,html,css,less,sass,scss,php,py,java,json,cson,xml,md,txt} --exclude-dir={node_modules,.venv,.env}'
